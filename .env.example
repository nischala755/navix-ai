# Navix-AI environment configuration
# Copy this file to .env and configure as needed

# Application
APP_NAME=Navix-AI
APP_VERSION=1.0.0
DEBUG=true
ENVIRONMENT=development

# Server
HOST=0.0.0.0
PORT=8000
WORKERS=1

# Database
DATABASE_URL=sqlite+aiosqlite:///./navix.db
DATABASE_ECHO=false

# HACOPSO Hyperparameters
HACOPSO_SWARM_SIZE=50
HACOPSO_MAX_ITERATIONS=200
HACOPSO_ARCHIVE_SIZE=100
HACOPSO_W_MAX=0.9
HACOPSO_W_MIN=0.4
HACOPSO_C1=2.0
HACOPSO_C2=2.0
HACOPSO_CHAOS_TYPE=logistic

# Ocean Grid
OCEAN_GRID_RESOLUTION=0.5
OCEAN_GRID_TIME_STEP=6

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60

# Background Tasks
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT_SECONDS=300
